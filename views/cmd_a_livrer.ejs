</head>
<body>
    
        <p id="hidenid">1</p>
       
    
    <h1>Bienvenue , Antonio!</h1>
    
    <a href="/index" id="index" >Accueil</a>
    <a href="/cmd_livrer" id="cmd_livrer" >Commande livrer</a>

    

    <script>
        document.addEventListener('DOMContentLoaded', () => {
      const hidenid = document.getElementById('hidenid').innerHTML;
      const userId = hidenid ;
      document.getElementById("hidenid").style.display = "none"
      const userLinkhistory = document.getElementById('index');
      userLinkhistory.href = `/index?userId=${encodeURIComponent(userId)}`;
      const userLinkproduis = document.getElementById('cmd_livrer');
      userLinkproduis.href = `/cmd_livrer?userId=${encodeURIComponent(userId)}`;
    });
        </script>
</body>
